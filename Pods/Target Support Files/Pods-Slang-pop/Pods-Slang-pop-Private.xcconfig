#include "Pods-Slang-pop.xcconfig"
CLANG_CXX_LANGUAGE_STANDARD = ${PODS_SLANG_POP_CLANG_CXX_LANGUAGE_STANDARD}
CLANG_CXX_LIBRARY = ${PODS_SLANG_POP_CLANG_CXX_LIBRARY}
CONFIGURATION_BUILD_DIR = $PODS_FRAMEWORK_BUILD_PATH
FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/pop" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Cartography" "${PODS_ROOT}/Headers/Public/Nimble" "${PODS_ROOT}/Headers/Public/Quick" "${PODS_ROOT}/Headers/Public/Reveal-iOS-SDK" "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = ${PODS_SLANG_POP_OTHER_LDFLAGS} -ObjC
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Slang
PODS_ROOT = ${SRCROOT}